<div class="login">
	<%= form_for :user, {url:"/auths", method:"post"} do |u|%>
	<%= u.text_field :username, :placeholder => "Username", class: 'form-control' %><br>
	<%= u.password_field :password, :placeholder => "Password", class: 'form-control' %><br>
	<%= u.submit "Log In", class: 'btn btn-primary' %>
	<%end%>
</div>
